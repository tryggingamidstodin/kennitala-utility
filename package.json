{
  "name": "kennitala-utility",
  "version": "5.1.1",
  "description": "Utility functions for icelandic social security numbers (kennitolur).",
  "main": "js/index.js",
  "types": "js/index.d.ts",
  "scripts": {
    "ci": "npm run clean && npm run compile && npm run test",
    "lint": "eslint -c eslint.config.js .",
    "lintfix": "eslint -c eslint.config.js --fix .",
    "format": "prettier --write 'src/*.ts'",
    "clean": "rm -rf js",
    "compile": "tsc",
    "build": "npm run clean && npm run compile",
    "test": "mocha js/*.spec.js",
    "tdd": "mocha --exit -R min --require ts-node/register --watch --inline-diffs src/*.ts",
    "prepublishOnly": "npm run ci"
  },
  "keywords": [
    "kennitala",
    "kennitolur",
    "kennit√∂lur",
    "identitynumber"
  ],
  "author": "Tryggingamidstodin <bt.hugbunadargerd@tm.is> (https://github.com/tryggingamidstodin/)",
  "files": [
    "js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format",
      "pre-push": "npm run ci"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.16",
    "chai": "^4.3.4",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^9.0.10",
    "mocha": "^10.2.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tryggingamidstodin/kennitala-utility"
  },
  "dependencies": {
    "oops-error": "^2.5.0"
  }
}
